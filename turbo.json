{
  "$schema": "https://turbo.build/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".svelte-kit/**", ".vercel/**"],
      "cache": false
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "check": {
      "cache": false
    }
  }
}
